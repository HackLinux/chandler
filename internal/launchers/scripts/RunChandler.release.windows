@echo off
setlocal
if "%CHANDLERHOME%"=="" set CHANDLERHOME=%cd%
if "%CHANDLERBIN%"=="" set CHANDLERBIN=%CHANDLERHOME%
if not defined OPTIMIZE set OPTIMIZE=-O
cd "%CHANDLERHOME%"
path %CHANDLERBIN%\release\bin
set LC_PATH=%CHANDLERHOME%\release\share\locale
"%CHANDLERBIN%"\release\bin\python.exe %OPTIMIZE% "%CHANDLERHOME%"\Chandler.py %*
endlocal
