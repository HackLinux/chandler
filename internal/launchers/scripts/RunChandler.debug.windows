@echo off
set RC=
setlocal
if "%CHANDLERHOME%"=="" call :dirname "%~dp0."
if "%CHANDLERBIN%"=="" set CHANDLERBIN=%CHANDLERHOME%

call "%~dp0\RunPython.bat" "%CHANDLERHOME%\Chandler.py" %*
goto end

:dirname
set CHANDLERHOME=%~dp1
goto end

:end
endlocal & set RC=%ERRORLEVEL%
goto omega

:returncode
exit /B %RC%

:omega
call :returncode %RC%
